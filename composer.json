{
    "name": "novembre/composer-input",
    "description": "Build HTML inputs for Wordpress ExPress Theme.",
    "keywords": ["input", "express", "package"],
    "license": "DBAD",
    "authors": [
        {
            "name": "Novembre Communication",
            "email": "contact@novembre.com"
        }
    ],
    "type": "package",
    "require": {
        "php": ">=5.4"
    },
    "autoload": {
        "psr-4": {
            "Novembre\\Input\\": "src/"
        }
    },
    "homepage": "https://github.com/gitnovembre/composer-input",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gitnovembre/composer-input",
            "options": {
                "ssh2": {
                    "username": "git-novembre",
                    "pubkey_file": "~/.ssh/id_rsa.pub",
                    "privkey_file": "~/.ssh/id_rsa"
                }
            }
        }
    ],
    "require-all": true
}
